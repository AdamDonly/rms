@import "reset.css";

html {height:105%;background-color:#fff;}
body { padding: 0px; color: #000; margin: 0px; 
       text-align: center; font: 12px 'Open Sans', serif;}

/* ------------------------------------------------ Font-FAMILY --- */
body {font-family:'Open Sans', serif;}

input, textarea, select, .brd_div_form {}
.listTable {}

/* ----------------------------------------------- Regular Links ---*/
#quickMenu a {padding:0 0.1em;text-decoration:none;color:#38348b; }
a {color:#38348b; }
a:hover, p:hover a:visited {text-decoration:underline;color:#df002c;list-style-type:color:#df002c;margin-top:0px;}

.tbLink { display: normal; text-decoration: none; padding: 3px; color: #555555; }
.tbLink a {padding:0 0.1em;text-decoration:none;color:#000;}
.tbLink a:hover, p:hover a:visited {text-decoration:underline;color:#df002c;list-style-type:color:#df002c;margin-top:10px;}

.quickLinks { display: block; text-decoration: none; padding: 3px 3px 3px 0px; color: #1b1b1b; font-size:105% }
.quickLinks.active { font-weight: bold; text-decoration: underline; color: #df002c; }
/* --------------------------------------------------- Headings --- */
h1, h2, h3, h4, h5, h6 {
  margin:1em 0 0.5em 0;line-height:1.2;font-weight:normal;}
h1 {font-size:250%;}
h2 {font-size:180%;}
h3 {font-size:140%;}
h4 {font-size:120%;}
h5 {font-size:110%;}
h6 {font-size:100%;}

/* ------------------------------------- paragraph, small, etc. --- */
p {margin-bottom:0.5em;line-height:1.5;font-weight:normal;font-size:100%;}
p.userName {text-align:right;line-height:15px;margin:0;}
small {font-size:85%;}
address {font-style:normal;}

/* -------------------------------------- align, spacing, other --- */
.spc00 {letter-spacing:0;}
.spc06 {letter-spacing:.6em;}
