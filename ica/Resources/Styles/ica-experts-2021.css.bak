/* --------------------------------------------------- HOME Page ---*/
div.section {display: block;padding: 5px 5px 2px 10px;border-bottom:1px solid #CCCCCC;background:#FFFFFF;font-weight:bold;font-size:12px;color:#666666;}
.section h2 {margin:0 0 0 0;padding:0 0 0 20px;font-weight:bold; line-height:60px;font-size:120%;color:#666;}
ul.section {list-style-type:square;color:#ff6666;margin-top:10px;}
