/* ======================================= DIVISION's structure === */
#header, #pageContent, #footer_wrap {position:relative;width:1280px; margin:0px auto 5px auto;text-align:left;clear:both;}
#header { background: #FFF; margin-bottom:10px; border-bottom: 1px solid #eee;line-height:14px;position:relative;min-height:83px;z-index:999;}
.headerlink {position:absolute;top:-1px;left:0px;width:280px;height:82px;margin-right:5px; background: url("/Resources/images/logo.png") left top no-repeat; cursor: pointer; }
.headerlink.ica-logo-small {width:112px;height:53px;background:url("/Resources/images/header-small.png") left top no-repeat;}
.assortisLogo {position:absolute;top:27px;left:200px;width:190px;height:43px;background: url("/Resources/images/logo-assortis.png") left top no-repeat; cursor: pointer;}
#pageContent {padding:0 20px 20px 20px;background-color:#fff;z-index:1;}
#footer {
  position:relative;width:100%;margin:0 0 0 0;padding:0;
  text-align:left;clear:both;
 
  border-top:5px #e0e0e0 solid;}

/* ------------------------------------------------- QUICK menu --- */
.user-quick-topmenu { padding:5px 0 20px 0; }
.hdr-icon { position: absolute;top:-10px; }
.hdr-info_item {display: inline-block;color:#FFF;font-weight:400;}
.hdr-info_item:last-child {padding-right:10px;}
.sep {display: inline-block;width: 1px;height: 15px;background: #FFF;opacity: 0.5;line-height: 26px;margin: 6px 17px;vertical-align: middle;}
.hdr-info_item .hdr-nav_a {color:#FFF;text-decoration:none;font-weight:600;}
.hdr-navitem { position: relative;}
.hdr-navitem_subNav {    list-style: none;display: none;padding-left: 10px;position: absolute;background: rgba(247,247,247,1);left: -16px;opacity: 0.8;}
.hdr-navitem_subNav li {position: relative;display: block;z-index: 99999999;margin: 0 !important;text-align: left;line-height: 30px;background: #F7F7F7;padding: 0 20px 0 5px;}
.hdr-navitem_subNav li a {line-height: 30px;white-space: nowrap;line-height:30px !important;}

ul#quickMenu {
  overflow:hidden;float:right;height:auto;margin:0;padding:0;
  list-style-type:none;font-size: 1em;font-weight:normal;}
#quickMenu li {
  float:left;margin:0 20px 0 2px;padding:0; line-height:14px;color:#FFF;font-size: 12px;font-weight: 800;letter-spacing: -0.12px; }
#quickMenu li a {line-height: 14px; color:#FFF;padding: 0 18px 0 18px; }
#quickMenu li a:hover {text-decoration:underline}
#quickMenu li.active a {text-decoration:underline;color:#df002c;}
ul#quickMenu li a.icapublic-link {background:url('/resources/images/earth-h14.png') no-repeat 1px 0;display:inline-block;height:14px;vertical-align:bottom;padding-left:18px}
a.twitter-link {background:url('/resources/images/icon-twitter.png') no-repeat 2px 0;display:inline-block;height:14px;width:20px;vertical-align:bottom;}
a.linkedin-link {background:url('/resources/images/icon-linkedin.png') no-repeat 4px 0;display:inline-block;height:14px;width:20px;vertical-align:bottom;}

a.public, a.sitemap, a.logout {
  background:transparent url("/Resources/images/icon_public.png") 0 50% no-repeat;
  display:inline;margin:0;padding:0 0 0 30px;}
a.sitemap {background-image:url("/Resources/images/icon_sitemap.png");}
a.logout  {background-image:url("/Resources/images/icon_logout.png");}

/* -------------------------------------------------- MAIN menu --- */

#mainMenu {margin:0;padding:0;}
ul#mainMenu { padding:0;list-style-type:none; line-height:23px; text-transform:none;}
#mainMenu li {display: block; float: left; margin-left: 30px; }
#mainMenu a { display: block;text-decoration: none; color: #373737;text-transform: uppercase;font-size: 14px;line-height: 57px;font-weight: 500; }
#mainMenu a.btn-assortis { padding:0;height:30px;width:124px;background:url('/Resources/Images/Logo_clean_465x152.png') no-repeat 6px;padding-top: 20px; background-size: 80%}
#mainMenu li:last-child{} 
#mainMenu li.first a { }
#mainMenu li a:hover {color: #e97d49; }
#mainMenu li.home a {color:#e97d49;} 

.topMenu { }
.topSubMenu {}

