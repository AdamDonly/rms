.dialog { text-align: left;  margin: 8px; }
.dialog .header { display: block; padding: 0px 5px 2px 10px; border-bottom: 1px solid #CCCCCC; background: #FFFFFF; font-weight: bold;font-size: 140%; color: #666666; }
.dialog .body { display: block; padding: 5px 5px 10px 5px; background: #FFFFFF; }
.dialog .body ul { margin: 0px; padding: 0px 0px 0px 10px; }
.dialog .body ul li { line-height: 1.5em; list-style-image:url("/Resources/images/tr.png"); }
.dialog .body ul li a { text-decoration: none; color: #38348b; }
.dialog .body ul li a:hover { text-decoration: underline; color: #38348b; }/*red underlines df002c*/
.dialog .body ul li div { padding: 5px 5px 15px 15px; }
.dialog .body ul li div, .dialog .body ul li span { color: #000000; }
/* nested DIVs */
.dialog .body ul li div div { padding: 0px; }
/* nested ULs */
.dialog .body ul li ul { margin: 0px; padding: 0px 0px 0px 10px; }
.dialog .body ul li ul li { line-height: 1.5em; list-style-type: none; list-style-image: none; color: #FF6666; }
.dialog .body ul li ul li:before { content: "- \0020"; }


