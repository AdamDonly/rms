Sub DonorListLoadData()
	Dim strSQL, objRs, i
	Dim DonorListCount

	strSQL = "EXEC usp_DonorListSelect"
	Set objRs = Server.CreateObject("ADODB.Recordset")
	objRs.Open strSQL, objConn, adOpenStatic, adLockReadOnly
	DonorListCount = objRs.RecordCount

	aOrg = DonorListCount
	ReDim aOrgCode(DonorListCount)
	ReDim aOrgInfo(DonorListCount)
	ReDim aOrgMainDonor(DonorListCount)
	
	i = 0
	Do Until objRs.EOF 
		aOrgInfo(i) = objRs("orgNameEng")
		Dim sOrgAbbreviation
		sOrgAbbreviation = objRs("orgAbbreviation")
		If Len(aOrgInfo(i) & sOrgAbbreviation)> 135 Then 
			aOrgInfo(i) = CutStringInMenu(aOrgInfo(i), 135 - Len(sOrgAbbreviation), " ", "&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;")
		End If

		aOrgInfo(i) = sOrgAbbreviation & " - " & aOrgInfo(i)
		aOrgCode(i) = CLng(objRs("id_Organisation"))
		aOrgMainDonor(i) = Abs(CInt(objRs("orgMainDonor")))

		i = i + 1
		objRs.MoveNext
	Loop
	
	objRs.Close
	Set objRs = Nothing
End Sub

Function DonorNameByID(iDonorID)
	Dim i, sResult
	sResult = ""

	If IsArray(aOrgCode) And IsArray(aOrgInfo) Then
		i = LBound(aOrgCode)
		Do Until i > UBound(aOrgCode) Or Len(sResult) > 0
			If aOrgCode(i) = iDonorID Then 
				sResult = aOrgInfo(i)
			End If
			i = i + 1
		Loop
	End If
DonorNameByID = sResult
End Function
